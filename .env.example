# 皓月后端环境变量配置示例

# MongoDB连接字符串
MONGODB_URI=mongodb+srv://username:password@cluster.example.com/database?retryWrites=true&w=majority

# JWT密钥
JWT_SECRET=your_jwt_secret_here
JWT_REFRESH_SECRET=your_jwt_refresh_secret_here

# Alpha Vantage API密钥 (用于获取真实股票数据)
# 申请地址: https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=YOUR_ALPHA_VANTAGE_API_KEY_HERE

# Finnhub API密钥 (可选，用于获取额外的股票数据)
# 申请地址: https://finnhub.io/dashboard
FINNHUB_API_KEY=YOUR_FINNHUB_API_KEY_HERE

# 应用配置
NODE_ENV=development
PORT=5000
FRONTEND_URL=http://localhost:3000

# 日志配置
LOG_LEVEL=info

# 其他配置
APP_VERSION=1.0.0
